1791. Find Center of Star Graph

/*
Runtime: 228 ms, faster than 82.26% of C++ online submissions for Find Center of Star Graph.
Memory Usage: 67.3 MB, less than 82.66% of C++ online submissions for Find Center of Star Graph.
*/

class Solution {
public:
    int findCenter(vector<vector<int>>& edges) {
      if(edges[0][0] == edges[1][0]) return edges[0][0];
          if(edges[0][0] == edges[1][1]) return edges[0][0];
        if(edges[0][1] == edges[1][0]) return edges[0][1];
          if(edges[0][1] == edges[1][1]) return edges[0][1];
        return 0;
    }
};
